#include <iostream>
#include <cstdlib>
#include <ctime>
#include<windows.h>
using namespace std;
int main()
{
cout<<""
srand((unsigned int)time(NULL));
int number= (rand()% 100)+1;
int guess=0;

	cout<<"Number Guessing Game\n\n";

do{

	cout<<"Enter Guess (1-100):";
	cin>>guess;
	
	if (guess>number)
	cout<<"Guess Lower"<<endl;
	else if (guess<number)
	cout<<"Guess Higher"<<endl;
	else
	cout<<"You Won"<<endl;
	
} 	while (guess != number);
	
	
	
	return 0;
}
